@page "/{game}"
@using SkiaSharp.Views.Blazor
@using WebGal.Global

<SKGLView @onclick="OnClick" @onmousemove="OnMouseMove"
	style=@($"width: {SceneConfig.DefaultWidth}px; height: {SceneConfig.DefaultHeight}px; border: 2px solid;")
	EnableRenderLoop="true" IgnorePixelScaling="true" OnPaintSurface="OnPaintSurface" />

@* <audio controls autobuffer autoplay="true" loop="true">
	<source src="data:audio/wav;base64,@B64Audio"/>
</audio> *@

@* <audio controls="controls" autobuffer="autobuffer" autoplay="autoplay">
<source src="data:audio/wav;base64," />
</audio> *@

@foreach(var (_,audio) in _loopAudios)
{
	<audio autoplay="true" loop="true">
		<source src="@audio"/>
	</audio>
}

@foreach(var (_,audio) in _oneShotAudios)
{
	<audio autoplay="true">
		<source src="@audio"/>
	</audio>
}

<p>@_text</p>
<p>@_mousePos</p>
<p>@_clickPos</p>
<p>FPS: @_fps</p>

